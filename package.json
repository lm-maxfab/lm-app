{
  "name": "my-vite-preact-ts-app",
  "version": "0.0.0",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "node ./scripts/dev.mjs",
    "postdev": "vite",
    "start": "npm run dev",
    "_build": "node ./scripts/build.BAK.mjs",
    "build": "tsc && vite build",
    "postbuild": "node ./scripts/postbuild.mjs"
  },
  "dependencies": {
    "@types/ogl": "npm:ogl-types@^0.0.102",
    "can-autoplay": "^3.0.2",
    "ogl": "^0.0.104",
    "p5": "^1.5.0",
    "preact": "^10.11.3",
    "react-slick": "^0.29.0",
    "slick-carousel": "^1.8.1",
    "smoothscroll-polyfill": "^0.4.4",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.7",
    "@babel/plugin-transform-modules-umd": "^7.18.6",
    "@preact/preset-vite": "^2.5.0",
    "@types/can-autoplay": "^3.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.18",
    "@types/p5": "^1.4.3",
    "@types/prompts": "^2.4.2",
    "@types/smoothscroll-polyfill": "^0.3.1",
    "async-prompt": "^1.0.1",
    "chalk": "^5.2.0",
    "classnames": "^2.3.2",
    "eslint": "^8.30.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-config-standard-ts": "^21.0.0",
    "fs-extra": "^11.1.0",
    "jsdom": "^20.0.3",
    "moment": "^2.29.1",
    "node-fetch": "^3.3.0",
    "pretty": "^2.0.0",
    "prompts": "^2.4.2",
    "sass": "^1.57.1",
    "source-map-explorer": "^2.5.3",
    "terser": "^5.16.1",
    "typescript": "^4.9.4",
    "vite": "^4.0.3",
    "zip-dir": "^2.0.0"
  },
  "eslintConfig": {
    "ignorePatterns": [
      "/build",
      "/node_modules",
      "/vite.config.ts",
      "/src/global.d.ts",
      "/src/custom.d.ts",
      "/src/preact.d.ts",
      "/src/preload.ts",
      "/src/vite-env.d.ts",
      "/static"
    ],
    "extends": [
      "react-app",
      "standard-ts"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "@typescript-eslint/triple-slash-reference": "off",
      "@typescript-eslint/lines-between-class-members": "off",
      "react/jsx-closing-bracket-location": "off",
      "operator-linebreak": "off"
    }
  }
}
